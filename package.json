{
  "name": "lair-queue",
  "version": "0.0.0",
  "description": "React application running the CS198 Lair Queue",
  "scripts": {
    "test": "node node_modules/jest-cli/bin/jest.js",
    "start": "grunt serve"
  },
  "author": "Omar Diab",
  "dependencies": {
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.2.0",
    "grunt-react": "^0.10.0",
    "react": "^0.12.0",
    "react-router": "^0.10.2"
  },
  "devDependencies": {
    "grunt-contrib-connect": "^0.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "jest-cli": "^0.1.18",
    "react-tools": "^0.12.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/scripts/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  }
}
