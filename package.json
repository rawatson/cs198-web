{
  "name": "lair-queue",
  "version": "0.0.0",
  "description": "React application running the CS198 Lair Queue",
  "scripts": {
    "test": "./node_modules/grunt-cli/bin/grunt jest",
    "start": "./node_modules/grunt-cli/bin/grunt serve"
  },
  "author": "Omar Diab",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "dependencies": {
    "express": "^4.10.1",
    "react": "^0.12.0",
    "react-router": "^0.10.2"
  },
  "devDependencies": {
    "browserify": "^6.3.3",
    "gulp": "^3.8.10",
    "gulp-connect": "^2.2.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.1",
    "jest-cli": "^0.1.18",
    "jsxhint": "^0.5.0",
    "react-tools": "^0.12.0",
    "reactify": "^0.17.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/scripts/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  }
}
